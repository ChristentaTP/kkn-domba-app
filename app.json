{
  "expo": {
    "name": "kkn-domba-app",
    "slug": "kkn-domba-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.ico",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/Logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.christentatp.kkndombaapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-sqlite"
    ],
    "jsEngine": "jsc",
    "extra": {
      "eas": {
        "projectId": "xxx-xxx"
      }
    }
  }
}
